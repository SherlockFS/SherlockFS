{
  "name": "SherlockFS, conteneur de d√©veloppement",
  "image": "ghcr.io/devcontainers/templates/ubuntu:1.2.0",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers/features/github-cli:1": {},
    "ghcr.io/devcontainers-contrib/features/bash-command:1": {
      "command": "git clone https://github.com/SherlockFS/SherlockFS.git"
    },
    "ghcr.io/devcontainers-contrib/features/bash-command:1": {
      "command": "cd SherlockFS && git checkout dev"
    },
    "ghcr.io/devcontainers-contrib/features/bash-command:1": {
      "command": "sudo cp SherlockFS/.github/apt_sources_list/ubuntu-noble.list /etc/apt/sources.list.d/"
    },
    "ghcr.io/devcontainers-contrib/features/bash-command:1": {
      "command": "sudo ./SherlockFS/dependencies.sh --with-tests"
    },
     "ghcr.io/devcontainers-contrib/features/bash-command:1": {
      "command": "rm -rf SherlockFS"
    }
  }
}
